{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhooks/lemon-squeezy",
      "dest": "/api/webhooks/lemon-squeezy.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/users/me",
      "dest": "/api/users/me.js"
    },
          {
            "src": "/api/subscription/(.*)",
            "dest": "/api/subscription.js"
          },
    {
      "src": "/api/usage",
      "dest": "/api/usage/index.js"
    },
    {
      "src": "/api/usage/sync",
      "dest": "/api/usage/index.js"
    },
    {
      "src": "/api/migrate",
      "dest": "/api/migrate.js"
    },
    {
      "src": "/api/models",
      "dest": "/api/models.js"
    },
    {
      "src": "/api/models/preference",
      "dest": "/api/models/preference.js"
    },
    {
      "src": "/api/ai/call",
      "dest": "/api/ai/call.js"
    },
    {
      "src": "/api/admin/add-api-key",
      "dest": "/api/admin/add-api-key.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/daily-reset",
      "schedule": "0 0 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
